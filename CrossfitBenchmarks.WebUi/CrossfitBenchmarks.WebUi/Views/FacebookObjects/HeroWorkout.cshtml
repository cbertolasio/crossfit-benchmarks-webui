@{
    Layout = "~/Views/Shared/_FbObjectLayout.cshtml";
}

<h2>Hero WOD</h2>
<p>Whenver you use Every WOD to log a Hero WOD - that activity can show up in Facebook.</p>
<p>Some of the Hero WODs are named Daniel, JT, and Murph.</p>

@section meta{
    <meta property="og:type" content="everywod:hero_workout" />
    <meta property="og:url" content="@Url.AbsoluteAction("heroworkout","facebookobjects")"/>
    <meta property="og:title" content="Hero Workout" />
    <meta property="og:description" content="Users of EveryWOD can log Hero WODs." />
    }