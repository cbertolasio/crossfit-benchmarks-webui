@model TheGirlsViewModel

@{
    ViewBag.Title = "The Girls";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div id="theGirls-content">

        <h2>Review Your Log Entries for The Girls</h2>
        
        <ul id="wodItems" data-bind="foreach: wodList()" class="thumbnails">
            <li class="span4">
                <div class="thumbnail" style="min-height:140px;" data-bind="attr:{id:id}">
                    @Html.Partial("WodItem")
                </div>
            </li>
        </ul>

    </div>

@Html.Partial("AddLogEntry", new AddLogEntryViewModel())

<script type="text/javascript">
    @Html.ToJSVar("theGirlsViewModel", Model)
</script>