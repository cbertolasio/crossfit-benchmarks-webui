
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - Every WOD</title>
        <link href="~/favicon.ico?v=2" rel="shortcut icon" />
        <link rel="image_src" href="@Url.AbsoluteContent("~/content/images/ew-fbObject-x1500.jpg")" />
        <meta property="og:image" content="@Url.AbsoluteContent("~/content/images/ew-fbObject-x1500.jpg")" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        @Styles.Render("~/Content/themes/base/css")
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>
    <body>
        <div class="wrapper">
        <div  class="container clear-top">
                <div class="navbar navbar-inverse navbar-static-top">
                    <div class="navbar-inner">

                            <ul class="nav">
                                <li class="@Html.IsActiveLink("Index","Home")">
                                    <a href="/"><i class="icon-home icon-white"></i> Home</a>
                                </li>

                                <li class="dropdown">
                                    <a class="dropdown-toggle" id="drop1" role="button" href="#" data-toggle="dropdown"><i class="icon-edit icon-white"></i> Tools <b class="caret"></b></a>
                                    <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                        @Html.MenuLink("Summary","Index", "Dashboard")
                                        <li class="divider"></li>
                                        @Html.MenuLink("Benchmarks","Index", "Logger", new { @val = "benchmark" })
                                        @Html.MenuLink("The Girls","Index", "Logger", new { @val = "thegirls" })
                                        @Html.MenuLink("The Heroes","Index", "Logger", new { @val = "theheroes" })
                                        <li class="divider"></li>
                                        @Html.MenuLink("% Calculator","Index", "Calculator")
                                    </ul>
                                
                                </li>

                                <li class="dropdown">
                                    <a class="dropdown-toggle" id="drop2" role="button" href="#" data-toggle="dropdown">
                                       <i class="icon-user icon-white"></i>  <b class="caret"></b>
                                    </a>

                                    <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                        @Html.MenuLink("Profile","Profile", "Dashboard", new { @val = "benchmark" })
                                        @Html.MenuLink("Settings","Settings", "Dashboard", new { @val = "thegirls" })
                                        <li class="divider"></li>
                                        @Html.MenuLink("Sign Out","Logout", "Home")
                                    </ul>
                                </li>
                                
                            </ul>
                    </div>
                </div>
        

            <div class="row">

            <div class="span12">
                @RenderBody()
            </div>
            
            
            
            </div>
        </div>
        <div class="push"><!--//--></div>
            </div>
    @Html.Partial("footer")



        
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/jqueryval")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/crossfitbenchmarks")
        @RenderSection("scripts", required: false)

<!-- UserVoice JavaScript SDK (only needed once on a page) -->
<script>(function () {
var uv = document.createElement('script'); uv.type = 'text/javascript'; uv.async = true;
uv.src = '//widget.uservoice.com/D252wS8FPY766Rjby8Gkhw.js';
var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(uv, s)
})()</script>

<!-- A tab to launch the Classic Widget -->
<script>
    UserVoice = window.UserVoice || [];
    UserVoice.push(['showTab', 'classic_widget', {
        mode: 'full',
        primary_color: '#000000',
        link_color: '#b94a48',
        default_mode: 'feedback',
        forum_id: 197767,
        tab_label: 'Feedback & Support',
        tab_color: '#b94a48',
        tab_position: 'middle-right',
        tab_inverted: false
    }]);
</script>

<script>
  (function (i, s, o, g, r, a, m) {
    i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
      (i[r].q = i[r].q || []).push(arguments)
    }, i[r].l = 1 * new Date(); a = s.createElement(o),
    m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
  })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

  ga('create', 'UA-43575514-1', 'azurewebsites.net');
  ga('send', 'pageview');

</script>
    </body>
</html>
